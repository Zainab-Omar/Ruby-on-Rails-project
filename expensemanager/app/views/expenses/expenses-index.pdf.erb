<h1>Expenses Report</h1>

<table class="table table-bordered">
	<th>Amount</th>
	<th>Category</th>
	<th>Date</th>
	<% @expenses.each do |expense| %>
	  <tr>
	    <td ><%= expense.amount %></td>
	    <td ><%= expense.category.try(:name)  %></td>
	    <td ><%= expense.created_at.to_date %></td>
	  </tr>
	<% end %>
</table>